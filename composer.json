{
    "name": "lquintana/google-places-popular-times",
    "description": "Get google places with its own pupular time for Laravel >= 5.8",
    "license": "MIT",
    "authors": [
        {
            "name": "Luciano Quintana",
            "email": "lquintana0707@gmail.com",
            "homepage": "https://github.com/1quintana/"
        }
    ],
    "homepage": "https://github.com/1quintana/google-places-popular-times",
    "keywords": [
        "Laravel",
        "Google Places",
        "Popular Times",
        "Google",
        "Places",
        "Place Details"
    ],
    "require": {
        "illuminate/support": "~5|~6|~7",
        "friendsofphp/php-cs-fixer": "^2.16",
        "guzzlehttp/guzzle": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "LQuintana\\GooglePopularTime\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LQuintana\\GooglePopularTime\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LQuintana\\GooglePopularTime\\GooglePopularTimeServiceProvider"
            ],
            "aliases": {
                "GooglePopularTime": "LQuintana\\GooglePopularTime\\Facades\\GooglePopularTime"
            }
        }
    }
}
